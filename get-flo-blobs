#!/bin/sh
wget https://dl.google.com/dl/android/aosp/asus-flo-1537103-7039b5f9.tgz
wget https://dl.google.com/dl/android/aosp/broadcom-flo-1537103-5d0f4b74.tgz
wget https://dl.google.com/dl/android/aosp/qcom-flo-1537103-92417c98.tgz
for i in *-flo-*.tgz; do
	tar xf $i
done
mkdir junk
echo 'cat "$@"' >junk/more
chmod +x junk/more
export PATH=`pwd`/junk:$PATH
for i in extract-*-flo.sh; do
	echo -e "\nI ACCEPT" |./$i
done
rm -rf junk *-flo-*.tgz extract-*-flo.sh

#!/bin/sh
# https://developers.google.com/android/nexus/drivers
wget https://dl.google.com/dl/android/aosp/asus-flo-lrx22c-dcd79ad4.tgz
wget https://dl.google.com/dl/android/aosp/broadcom-flo-lrx22c-21096d16.tgz
wget https://dl.google.com/dl/android/aosp/qcom-flo-lrx22c-5aaadedc.tgz
for i in *-flo-*.tgz; do
	tar xf $i
done
mkdir junk
echo 'cat "$@"' >junk/more
chmod +x junk/more
export PATH=`pwd`/junk:$PATH
for i in extract-*-flo.sh; do
	echo -e "\nI ACCEPT" |./$i
done
rm -rf junk *-flo-*.tgz extract-*-flo.sh

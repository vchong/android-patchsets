#!/bin/bash
# https://dl.google.com/dl/android/aosp/linaro-hikey-20160226-67c37b1a.tgz
wget https://dl.google.com/dl/android/aosp/linaro-hikey-20160226-67c37b1a.tgz
for i in linaro-hikey-*.tgz; do
	tar xf $i
done
mkdir junk
echo 'cat "$@"' >junk/more
chmod +x junk/more
export PATH=`pwd`/junk:$PATH
for i in extract-linaro-hikey.sh; do
	echo -e "\nI ACCEPT" |./$i
done
rm -rf junk linaro-hikey-*.tgz extract-linaro-hikey.sh

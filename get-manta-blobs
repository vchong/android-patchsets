#!/bin/sh
wget https://dl.google.com/dl/android/aosp/audience-manta-lrx21p-d3749cac.tgz
wget https://dl.google.com/dl/android/aosp/broadcom-manta-lrx21p-23ffa32a.tgz
wget https://dl.google.com/dl/android/aosp/samsung-manta-lrx21p-0bbebdb4.tgz
for i in *-manta-*.tgz; do
	tar xf $i
done
mkdir junk
echo 'cat "$@"' >junk/more
chmod +x junk/more
export PATH=`pwd`/junk:$PATH
for i in extract-*-manta.sh; do
	echo -e "\nI ACCEPT" |./$i
done
rm -rf junk *-manta-*.tgz extract-*-manta.sh

#!/bin/sh
wget https://dl.google.com/dl/android/aosp/audience-manta-1537103-8f1a4c56.tgz
wget https://dl.google.com/dl/android/aosp/broadcom-manta-1537103-15b8bdff.tgz
wget https://dl.google.com/dl/android/aosp/samsung-manta-1537103-bcd253ac.tgz
for i in *-manta-*.tgz; do
	tar xf $i
done
mkdir junk
echo 'cat "$@"' >junk/more
chmod +x junk/more
export PATH=`pwd`/junk:$PATH
for i in extract-*-manta.sh; do
	echo -e "\nI ACCEPT" |./$i
done
rm -rf junk *-manta-*.tgz extract-*-manta.sh

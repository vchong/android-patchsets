#!/bin/sh
wget https://dl.google.com/dl/android/aosp/audience-manta-1502407-d1b40f29.tgz
wget https://dl.google.com/dl/android/aosp/broadcom-manta-1502407-70a0cfcc.tgz
wget https://dl.google.com/dl/android/aosp/samsung-manta-1502407-dd67b717.tgz
for i in *-manta-*.tgz; do
	tar xf $i
done
mkdir junk
echo 'cat "$@"' >junk/more
chmod +x junk/more
export PATH=`pwd`/junk:$PATH
for i in extract-*-manta.sh; do
	echo -e "\nI ACCEPT" |./$i
done
rm -rf junk *-manta-*.tgz extract-*-manta.sh
